.mem_rech {
  width: 100%;
  height: 100%;
  background: rgba(238, 238, 238, 1);
}

.mem_je {
  padding: 28rpx 40rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
}

.mem_je text {
  font-size: 32rpx;
  font-family: PingFang SC;
  color: rgba(51, 51, 51, 1);
}

.mem_ts {
  padding: 20rpx 40rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  color: rgba(154, 154, 154, 1);
}

.mem_logo {
  padding: 14rpx 40rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
}

.mem_logo image {
  width: 72rpx;
  height: 72rpx;
}

.mem_logo image.imgs {
  width: 48rpx;
  height: 48rpx;
  position: relative;
  top: 12rpx;
}

.mem_but {
  padding: 80rpx 40rpx;
}

button {
  width: 100%;
  height: 80rpx;
  background: rgba(41, 148, 230, 1);
  border-radius: 8rpx;
  margin: 0 auto;
  font-size: 36rpx;
  font-family: PingFang SC;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
}
