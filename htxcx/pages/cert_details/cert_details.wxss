.certBox {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.box {
  padding: 40rpx 40rpx 90rpx 40rpx;
  text-align: center;
}

.box_up {
  text-align: center;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  line-height: 50rpx;
  color: rgba(51, 51, 51, 1);
}

.box image {
  width: 430rpx;
  height: 600rpx;
  margin: 40rpx 0;
}

.box .text {
  font-size: 32rpx;
  font-family: PingFang SC;
  line-height: 44rpx;
  color: rgba(51, 51, 51, 1);
}
